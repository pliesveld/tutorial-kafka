[Unit]
Description=A sample project {{project.name}}
After=multi-user.target
After=pulseaudio.service
#After=ssh.service
After=syslog.target
After=network.target
Requires=pulseaudio.service
#Requires=ssh.service

[Service]
ExecStart=/usr/bin/java {{jvm_args}} -jar {{project.install_dir}}/tutorial-kafka.jar {{command}} {{shell_args}}
Type=simple
Restart={{systemd_service_restart}}
RestartSec=10800ms
StartLimitIntervalSec=0

[Install]
WantedBy=default.target
